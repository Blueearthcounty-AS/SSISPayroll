@model SSISPayroll.Models.SsisRoleBrassConversion

    <div class="modal-dialog">
        <div class="modal-content">
            <div class="row">
                <div class="col-md-12">
                    <form asp-action="Delete">
                        <div class="modal-header">
                            <h5 class="modal-title">Delete Brass Code Conversion</h5>
                            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                        </div>
                        <div class="modal-body">
                            <h6>Are you sure you want to delete this record?</h6>
                            <hr />
                            <div>
                                <dl class="row">
                                    <dt class="col-sm-4">
                                        @Html.DisplayNameFor(model => model.SsisRoleId)
                                    </dt>
                                    <dd class="col-sm-8">
                                        @Html.DisplayFor(model => model.SsisRoleId)
                                        <input type="hidden" asp-for="SsisRoleId" />
                                    </dd>
                                    <dt class="col-sm-4">
                                        @Html.DisplayNameFor(model => model.Brass)
                                    </dt>
                                    <dd class="col-sm-8">
                                        @Html.DisplayFor(model => model.Brass)
                                        <input type="hidden" asp-for="Brass" />
                                    </dd>
                                    <dt class="col-sm-4">
                                        @Html.DisplayNameFor(model => model.OrgCode)
                                    </dt>
                                    <dd class="col-sm-8">
                                        @Html.DisplayFor(model => model.OrgCode)
                                        <input type="hidden" asp-for="OrgCode" />
                                    </dd>
                                </dl>
                            </div>
                        </div>
                        <div class="modal-footer">                            
                            <input type="hidden" asp-for="SsisRoleBrassConversionId" />
                            <input type="submit" value="Yes" class="btn btn-dark btn-sm" />
                            <button type="button" class="btn btn-outline-dark btn-sm" data-dismiss="modal">No</button>                           
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>



