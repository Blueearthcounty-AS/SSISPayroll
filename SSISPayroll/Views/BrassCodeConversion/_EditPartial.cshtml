@model SSISPayroll.Models.SsisRoleBrassConversion

<div class="modal-dialog">
    <div class="modal-content">
        <div class="row">
            <div class="col-md-12">
                <form asp-action="Edit">
                    <div class="modal-header">
                        <h5 class="modal-title">Edit Brass Code Conversion</h5>
                        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                    </div>
                    <div class="modal-body">
                        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
                        <div class="form-group" hidden>
                            <label asp-for="SsisRoleBrassConversionId" class="control-label"></label>
                            <input asp-for="SsisRoleBrassConversionId" class="form-control" />
                            <span asp-validation-for="SsisRoleBrassConversionId" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="SsisRoleId" class="control-label"></label>
                            <input asp-for="SsisRoleId" class="form-control" />
                            <span asp-validation-for="SsisRoleId" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="Brass" class="control-label"></label>
                            <input asp-for="Brass" class="form-control" />
                            <span asp-validation-for="Brass" class="text-danger"></span>
                        </div>
                        <div class="form-group">
                            <label asp-for="OrgCode" class="control-label"></label>
                            <input asp-for="OrgCode" class="form-control" />
                            <span asp-validation-for="OrgCode" class="text-danger"></span>
                        </div>
                    </div>
                    <div class="modal-footer">
                        <button type="button" class="btn btn-outline-dark btn-sm" data-dismiss="modal">Close</button>
                        <input type="submit" value="Edit" class="btn btn-dark btn-sm" />
                    </div>
                </form>
            </div>
        </div>
    </div>
</div>



